  <div id="carouselExampleAutoplaying" class="carousel slide" data-bs-ride="carousel" data-bs-interval="false">
  <div class="carousel-inner">
    <% banners.forEach((banner, index) => { %>
      <% banner.bannerImage.forEach((image, imageIndex) => { %>
        <div class="carousel-item <%= index === 0 && imageIndex === 0 ? 'active' : '' %>">
          <div class="carousel-image-container">
            <img src="/uploads/<%= image %>" class="d-block w-100 carousel-image" alt="..." style="object-fit: cover;">
            <div class="carousel-caption text-center" style="color: #fff; position: absolute; top: 50%; left: 50%; transform: translate(-50%, -50%);"> <!-- Center align the caption -->
              <h5 id="bannerTitle"style="font-size: 4rem;" >
                <%= banner.bannerTitle %>
              </h5>
              <p id="bannerSubTitle" style="font-size: 2rem;">
                <%= banner.bannerSubTitle %>
              </p>
              <a href="<%= banner.bannerUrl %>">
                <button class="btn btn-primary">View Details</button>
              </a>
            </div>
          </div>
        </div>
      <% }); %>
    <% }); %>
