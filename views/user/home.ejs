  <script>
  // Function to start carousel autoplay
  function startCarouselAutoplay() {
    // Get the carousel element
    const carousel = document.getElementById('carouselExampleAutoplaying');
    // Initialize the carousel
    const myCarousel = new bootstrap.Carousel(carousel, {
      interval: 3000, // Adjust the interval as needed (in milliseconds)
      pause: false,   // Allow the carousel to keep cycling continuously
    });
  }

  // Start carousel autoplay when the DOM content is loaded
  document.addEventListener('DOMContentLoaded', startCarouselAutoplay);
</script>


